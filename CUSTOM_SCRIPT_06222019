function excludeScriptFromRunning()
{
	compareFileDate = new Date(lookup("EXCLUDE_DATE", "FileDate"));
	recordFileDate = new Date(fileDate);
	if(recordFileDate < compareFileDate)
	{
		return true;
	}
	else
	{
		return false;
	}
}
function feeInvoicedAlarms()
{
	var emailParams = aa.util.newHashtable();
	var reportParams = aa.util.newHashtable();
	var reportFile = new Array();
	var conArray = getContactArray();
	var conEmail = "";

	for (con in conArray)
	{
		if (!matches(conArray[con].email, null, undefined, ""))
		{
			conEmail += conArray[con].email + "; ";
		}
	}
	getRecordParams4Notification(emailParams);
	addParameter(emailParams, "$$applicationName$$", cap.getCapModel().getAppTypeAlias());
	if (conEmail != null)
	{
		sendNotification("noreply@fortlauderdale.gov", conEmail, "", "ALM-FEES INVOICED", emailParams, reportFile);
	}
}

