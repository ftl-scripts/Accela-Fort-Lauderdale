				{
					gsArray = gs.toArray();
					for (var loopk in gsArray)
					{
						var guideType = gsArray[loopk].getGuideType();
						if(guideType.contains("ENF_Violation Checklist"))
						{
							var gsItems = gsArray[loopk].getItems().toArray();
